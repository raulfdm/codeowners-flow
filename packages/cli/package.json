{
  "name": "codeowners-js",
  "version": "0.0.1",
  "description": "A CLI to manage CODEOWNERS file",
  "keywords": [
    "codeowners",
    "github",
    "git"
  ],
  "author": "Raul Melo",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "index.d.ts",
    "config.d.ts"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/src/index.d.ts"
    }
  },
  "scripts": {
    "prepare": "pnpm turbo build --filter codeowners-js --output-logs errors-only",
    "build": "vite build",
    "dev": "tsx src/index.ts",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@codeowners-js/config": "workspace:*",
    "@manypkg/find-root": "2.2.1",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@codeowners-js/eslint": "workspace:*",
    "@types/node": "16",
    "eslint": "8.47.0",
    "tsx": "3.12.7",
    "typescript": "5.1.6",
    "vite": "4.4.8",
    "vite-plugin-dts": "3.5.1"
  }
}
